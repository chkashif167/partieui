<div class="modal preference-modal dynamic-height" id="{{ modalId }}">
    <div class="modal-overlay"></div>
    <div class="modal-container">
        <div class="modal-header">
            <button class="btn bg-black btn-close" onclick="closeModal()">
                <img src="/images/icons/misc/back-carat.svg" alt="Back Icon">
            </button>
        </div>
        <div class="modal-content">
            <h2>{{ modalTitle }}</h2>
            {{#modalOptions}}
                {{> components-checkbox-group }}
            {{/modalOptions}}
        </div>
    </div>
</div>
